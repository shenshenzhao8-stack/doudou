<view class="occlusion-layer" tt:if="{{internalVisible}}" catchtap="handleCancelTap">
  <view class="content" catchtap="noop">
    <view class="cancel">
      <image src="/assets/guanbi.png" bindtap="handleCancelTap" class="cancel-image"></image>
    </view>

    <view class="title">用户协议及隐私</view>

    <view class="rule">已阅读并同意 <text bindtap="handleRouterTap">抖音商城上新了小程序隐私协议</text>
    </view>

    <button bindtap="handleTap">同意并授权</button>
  </view>
</view>
