.swiper-box {
    position: relative;
    width: 100%;
    height: 958rpx;
    overflow: hidden;
}

.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    height: 739rpx;
    position: absolute;
    width: 100%;
    left: 0;
    right: 0;
    top: 192rpx;
    z-index: 9;

}


.swiper-container {
    position: relative;
    width: 100%;
    height: 739rpx;
    margin-top: 20rpx;
    overflow: hidden;
}




.swiper-box::after {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    backdrop-filter: blur(10px);
    pointer-events: none;
}

.calendar-swiper {
    width: 100%;
    height: 100%;
}

.swiper-item {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100%;
    border-top-left-radius: 20rpx !important;
    border-top-right-radius: 20rpx !important;
    overflow: hidden;
    position: relative;
}



.item-content-wrapper {
    width: 100%;
    height: 100%;
    overflow: hidden;
    border-radius: 20rpx;
    position: relative;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: flex-end;
    padding-bottom: 0rpx;
    transform: translateY(25rpx) rotateX(8deg) scale(0.95);
    transition: all 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
    transform-style: preserve-3d;
}

.item-content-wrapper.active {
    transform: translateY(0) rotateX(0deg) scale(1);
}


