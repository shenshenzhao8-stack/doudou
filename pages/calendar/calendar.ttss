/* slide 动画 */
.slide-animate {
    animation: slide cubic-bezier(0.7, 0, 0.3, 1) 1s 1;
}

@keyframes slide {
    from {
        transform: translateY(20rpx);
        opacity: 0;
    }

    50% {
        opacity: 0.5;
    }

    to {
        transform: translateY(0px);
        opacity: 1;
    }
}

/* slide 动画 */
.slideOut-animate {
    animation: slideOut cubic-bezier(0.7, 0, 0.3, 1) .5s 1;
}

@keyframes slideOut {
    from {
        opacity: 1;
        transform: translateX(0px);
    }

    50% {
        opacity: 0.5;
    }

    to {
        opacity: 0;
        transform: translateX(-200rpx);
    }
}

/* slide 动画 */
.slideIn-animate {
    animation: slideIn cubic-bezier(0.7, 0, 0.3, 1) .5s 1;
}

@keyframes slideIn {
    from {
        opacity: 0;
        transform: translateX(200px);
    }

    50% {
        opacity: 0.5;
    }

    to {
        opacity: 1;
        transform: translateX(0px);
    }
}


.cover {
