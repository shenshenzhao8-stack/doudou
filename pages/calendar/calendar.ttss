.tabs-scroll {
    width: 100%;
}

.tabs {
    box-sizing: border-box;
    display: flex;
    padding: 0px 28rpx;
    width: 100%;
    justify-content: space-between;
    overflow-x: scroll;
    height: 113rpx;
    padding-top: 19rpx;
    align-items: center;
    background-color: #fff;
}

/* Tab 项基础样式 */
.tabs-item {
    position: relative;
    width: 132rpx;
    height: 113rpx;
    display: inline-block;
    transition: all 0.3s;
}

.calendar-zh {
    font-weight: bold;
    font-size: 26rpx;
    line-height: 45rpx;
    text-align: center;
    /* text-shadow: #A487E2;
    -webkit-text-stroke: #A487E2; */
}

.calendar-cn {
    font-weight: 400;
    font-size: 22rpx;
    line-height: 40rpx;
    text-align: center;
    margin-top: -10rpx;

}

.tabs-item-month {
    position: absolute;
    left: 0;
    top: 0;
    width: 132rpx;
    height: 113rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    z-index: 9;

}

.month-bg {
    display: block;
    width: 134rpx;
    height: 113rpx;
}

/* 激活状态样式 */
.tabs-item-active {
    color: #fff;
    font-weight: bold;
    text-shadow: 0px 0px 9px rgba(0, 0, 0, 0.51);
}

