.noshop {
    margin: 0 auto;
    transform: scale(0.7);
}

.noshop-img {
    display: block;
    width: 500rpx;
    height: 400rpx;
    margin: 0 auto;
}

.noshop-tip-title {
    display: block;
    width: 100%;
    text-align: center;
    font-size: 30rpx;
    color: #333;
}

.no_data_tip {
    display: block;
    width: 100%;
    text-align: center;
    font-size: 24rpx;
    color: #333;
    padding: 10rpx 0 50rpx;
    background-color: #F6F5F8;
}


.cover {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    background: rgba(156, 132, 219, 1);
    opacity: 0;
    transition: opacity 2s cubic-bezier(0.33, 1, 0.68, 1);
    z-index: 999;
}

.cover-show {
    opacity: 1;
}




/* 弹窗 */
.dialog {
    position: fixed;
    width: 100vw;
    height: 100vh;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, .6);
    display: flex;
    justify-content: center;
    align-items: center;
    overflow: hidden;
    z-index: 10;
}
.content{
    /* display: flex;
    flex-direction: column; */
    /* background-color: red; */
}
.content .logo {
    display: block;
    width: 580rpx;
    height: 712rpx;
}

.cancel {
    display: flex;
    justify-content: center;
    margin-top: 60rpx;
    z-index: 99999;
}

.dialof-close-btn {
    height: 68rpx;
    width: 68rpx;
}
.index-dialog-img {
    display: block;
    width: 525rpx;
    height: 628.5rpx;
}

.custom-icon {
    position: fixed;
    display: block;
    width: 140px;
    height: 30px;
    z-index: 999;
}
.index-bannner-default{
    display: block;
    width: 100%;
    height: 476rpx;
}