.page-index {
    /* height: 100vh; */
    position: relative;
    background-color: #F6F5F8;
}

.page-content {
    border-top-right-radius: 20rpx;
    border-top-left-radius: 20rpx;
    position: absolute;
    height: 1000rpx;
    width: 100vw;
    left: 0;
    top: 443rpx;
    z-index: 2;
    background-color: #F6F5F8;

}



.menu-calendar {
    width: 350rpx;
    height: 168rpx;
    background: linear-gradient(138deg, #FDE3F5 0%, #DFC3FB 71%, #C9ACFE 100%);
    border-radius: 20rpx;
}

.menu-new {
    width: 350rpx;
    height: 168rpx;
    background: linear-gradient(225deg, #77E2F6 0%, #A6FF9C 58%, #D5F4CB 100%);
    border-radius: 20rpx;
}


.suspension-box {
    position: fixed;
    width: 200rpx;
    height: 200rpx;
    z-index: 99;
    right: 20rpx;
    bottom: 103rpx;
}

.page-menu {
    border-top-right-radius: 20rpx;
    border-top-left-radius: 20rpx;
    width: 100%;
    padding: 0 59rpx;
    padding-bottom: 24rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: space-between;
    background-color: #fff;
    padding-top: 24rpx;
    overflow: hidden;

}

/* Tab 项基础样式 */
.tabs-item {
    position: relative;
    display: flex;
    transition: all 0.3s;
    flex-direction: column;
    align-items: center;
}

.tabs-title {
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 24rpx;
    color: #fff;
    font-weight: bold;
    line-height: 34rpx;
    text-align: center;
    width: 132rpx;
    height: 34rpx;
    background: linear-gradient(270deg, #8967D9 0%, #987ADB 38%, #BB97FA 100%);
    border-radius: 17rpx;
}

.icon_arrow {
    display: block;
    width: 16rpx;
    height: 16rpx;
}


.item-icon {
    display: block;
    width: 140rpx;
    height: 140rpx;
    /* animation: move 2s ease infinite */
}

.item-gif {
    display: block;
    width: 140rpx;
    height: 140rpx;
}

